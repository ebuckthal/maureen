{
  "name": "maureen",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ebuckthal.github.io/maureen",
  "dependencies": {
    "autoprefixer": "^9.4.3",
    "gh-pages": "^2.0.1",
    "prettier": "^1.15.3",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-scripts": "2.1.2"
  },
  "scripts": {
    "build:css": "postcss src/app.css -o src/index.css",
    "watch:css": "postcss src/app.css -o src/index.css -w",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "start": "yarn watch:css & react-scripts start",
    "build": "yarn build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "postcss-cli": "^6.1.0",
    "tailwindcss": "^0.7.3"
  }
}
